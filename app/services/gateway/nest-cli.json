{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "typeCheck": false,
    "builder": {
       "type": "swc",
       "options": {
          "swcrcPath": "./.swcrc"
       }
    }
  }
}
